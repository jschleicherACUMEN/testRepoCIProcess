<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>VIEWS_Date_Signed_Required_CL</fullName>
        <active>true</active>
        <description>the Date Signed field must be filled before a Congressional Letters RT case can be closed. An error message displays if the user tries closing the case without filling the Date Signed field.</description>
        <errorConditionFormula>AND( 
$RecordType.DeveloperName=&apos;Other&apos;, 
IF(ISPICKVAL( Status , &apos;Closed&apos;), 
ISBLANK(Views_Date_Signed_Wet__c) , false) 
)</errorConditionFormula>
        <errorDisplayField>Views_Date_Signed_Wet__c</errorDisplayField>
        <errorMessage>Date Signed field must be filled before the case can be closed</errorMessage>
    </validationRules>
</CustomObject>
